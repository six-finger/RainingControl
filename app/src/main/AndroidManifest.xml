<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rainingControl">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".frame.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".frame.HistoryActivity" />
        <activity android:name=".frame.CityActivity" />
        <activity android:name=".frame.CatchActivity" />
        <activity android:name=".frame.LidActivity" />
        <activity android:name=".frame.CatchAddActivity" />
        <activity android:name=".frame.CatchRemoveActivity" />
        <activity android:name=".frame.ResultActivity"/>
    </application>

</manifest>